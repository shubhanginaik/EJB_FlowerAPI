<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>All flowers</title>
    <link rel="stylesheet" href="/styles/styles.css">
</head>
<body>
    <main>
        <h1>All Flowers</h1>
        <table>
            <thead>
                <tr>
                    <th>Flower Id</th>
                    <th>Name</th>
                    <th>Stock</th>
                    <th>Site</th>
                    <th>Farmer</th>
                </tr>
            </thead>
            <tbody>
                <% result.forEach(flower => { %>
                    <tr>
                        <td><%= flower.flowerId %></td>
                        <td><%= flower.name %></td>
                        <td><%= flower.stock %></td>
                        <td><%= flower.site %></td>
                        <td><%= flower.farmer %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
        <a href="/"><button>Menu</button></a>
    </main>
</body>
</html>